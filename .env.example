# Google API Key (used to access Google APIs)
GOOGLE_API_KEY=your_google_api_key_here

# Gemini API endpoint (example: for calling the Gemini generative AI model)
GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent

# OpenRouter API Key
# This key is used to authenticate with the OpenRouter API for generating quizzes.
# You can obtain your API key from your OpenRouter dashboard: https://openrouter.ai/settings
OPENROUTER_API_KEY="sk-or-your-actual-api-key-here"

# OpenRouter API URL
# This is the endpoint for the OpenRouter chat completions API.
# It typically points to the v1 chat completions endpoint.
OPENROUTER_API_URL="https://openrouter.ai/api/v1/chat/completions"